<project name="${project.name}">

    <bannerLeft>
        <name>${project.name}</name>
        <src>images/logo_horizontal.png</src>
        <href>http://cjwizard.com/</href>
    </bannerLeft>
    <bannerRight>
    
        <src>images/icononly.png</src>
    </bannerRight>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.6</version>
    </skin>

    <publishDate format="dd MMM yyyy" position="right"/>

    <body>
        <breadcrumbs>
            <item name="${project.name}" href="https://github.com/cjwizard"/>
        </breadcrumbs>
        <menu >
            <item name="Project Information" href="index.html"/>
            <item name="Quick Start" href="quickstart.html"/>
            <item name="Design" href="design.html"/>
            <item name="Event Sequence" href="eventsequence.html"/>
            <item name="FAQ" href="FAQ.html"/>
	  		
        </menu>
        <menu ref="parent" inherit="top"/>
        <menu ref="modules" />
        <menu ref="reports"  inherit="top"/>
        <footer></footer>
    </body>
    <custom>
        <fluidoSkin>
            <gitHub>
                <projectId>cjwizard/cjwizard</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>green</ribbonColor>
            </gitHub>
            <topBarEnabled>false</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
            <googleSearch>
                <sitesearch>https://cjwizard.github.io/</sitesearch>
            </googleSearch>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
        </fluidoSkin>
    </custom>
</project>

